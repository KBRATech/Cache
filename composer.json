{
    "name": "kbra/cache",
    "description": "KBRA Cache",
    "type": "project",
    "license": "Apache 2.0",
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.kbra.com"
        }
    ],
    "require": {
        "phpfastcache/phpfastcache": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "vimeo/psalm": "^1"
    },
    "autoload": {
        "psr-4": {
            "Kbra\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kbra\\Cache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
